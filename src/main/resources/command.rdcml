    hook skull
}

troll player:target{
    help /troll <user>
    user player
    permission tazpvp.troll
    demo {
        postarg
        hook demo
    }
}

npc {
    help /npc
    user player
    permission tazpvp.npc
    create string:name string:profession string:type int:id int:entity int:pitch{
        help create a npc
        hook npc_create
        postarg
    }
    remove string:name{
        help remove a npc
        hook npc_remove
        postarg
    }
    rename string:name string:New{
        help rename an npc
        hook npc_rename
        postarg
    }
    list {
        help list all npcs
        hook npc_list
        postarg
    }
}

