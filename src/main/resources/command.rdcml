stats {
    permission tazpvp.admin
    help /stats <points|exp|expleft|level|money|deaths|kills|streak>
    shards {
        help /stats shards <add|remove|set|reset|get> <player> [amount]
        add player:target int:amount {
            hook shard_add
        }
        remove player:target int:amount {
            hook shard_remove
        }
        set player:target int:amount {
            hook shard_set
        }
        reset player:target {
            hook shard_reset
        }
        get player:target {
            hook shard_get
        }
    }
    exp {
        help /stats exp <add|remove|set|reset|get> <player> [amount]
        add player:target int:amount {
            hook exp_add
        }
        remove player:target int:amount {
            hook exp_remove
        }
        set player:target int:amount {
            hook exp_set
        }
        reset player:target {
            hook exp_reset
        }
        get player:target {
            hook exp_get
        }
    }
    level {
        help /stats level <add|remove|set|reset|get> <player> [amount]
        add player:target int:amount {
            hook level_add
        }
        remove player:target int:amount {
            hook level_remove
        }
        set player:target int:amount {
            hook level_set
        }
        reset player:target {
            hook level_reset
        }
        get player:target {
            hook level_get
        }
    }
    coins {
        help /stats money <add|remove|set|reset|get> <player> [amount]
        add player:target int:amount {
            hook coins_add
        }
        remove player:target int:amount {
            hook coins_remove
        }
        set player:target int:amount {
            hook coins_set
        }
        reset player:target {
            hook coins_reset
        }
        get player:target {
            hook coins_get
        }
    }
    deaths {
        help /stats deaths <add|remove|set|reset|get> <player> [amount]
        add player:target int:amount {
            hook deaths_add
        }
        remove player:target int:amount {
            hook deaths_remove
        }
        set player:target int:amount {
            hook deaths_set
        }
        reset player:target {
            hook deaths_reset
        }
        get player:target {
            hook deaths_get
        }
    }
    kills {
        help /stats kills <add|remove|set|reset|get> <player> [amount]
        add player:target int:amount {
            hook kills_add
        }
        remove player:target int:amount {
            hook kills_remove
        }
        set player:target int:amount {
            hook kills_set
        }
        reset player:target {
            hook kills_reset
        }
        get player:target {
            hook kills_get
        }
    }
    streak {
        help /stats streak <add|remove|set|reset|get> <player> [amount]
        add player:target int:amount {
            hook streak_add
        }
        remove player:target int:amount {
            hook streak_remove
        }
        set player:target int:amount {
            hook streak_set
        }
        reset player:target {
            hook streak_reset
        }
        get player:target {
            hook streak_get
        }
    }
    spins {
       add player:target int:amount {
           hook spins_add
       }
       remove player:target int:amount {
           hook spins_remove
       }
       set player:target int:amount {
           hook spins_set
       }
       reset player:target {
           hook spins_reset
       }
       get player:target {
           hook spins_get
       }
    }
    credits {
           add player:target int:amount {
               hook credits_add
           }
           remove player:target int:amount {
               hook credits_remove
           }
           set player:target int:amount {
               hook credits_set
           }
           reset player:target {
               hook credits_reset
           }
           get player:target {
               hook credits_get
           }
        }
    reset player:target {
        hook stats_reset
    }

    scoreboard {
        hook stats_scoreboard
        user player
    }
}
gui string:gui {
    permission tazpvp.admin
    help /gui [guiname]
    user player
    hook gui
}

spawn player:target?(context self){
    help /spawn
    user player
    hook spawn
}

warn player:target string...:reason {
    permission tazpvp.warn
    help /warn <player> <reason>
    user player
    hook warn
}

mutechat,unmutechat {
    permission tazpvp.mutechat
    help /mutechat
    hook mutechat
}
mute player:target {
    permission tazpvp.mute
    help /mute <player>
    hook mute
}
unmute player:target {
    permission tazpvp.mute
    help /unmute <player>
    hook unmute
}
ench string:enchantment int:level?(1) {
    user player
    permission tazpvp.enchant
    help /ench <enchantment> [level]
    hook enchant
}
world {
    user player
    permission tazpvp.world
    help /world <list|tp> <world>
    list {
        hook world_list
    }
    tp world:world {
        hook world_tp
    }
}
items {
    permission tazpvp.items
    help items command yes
    user player
    give {
        help gives the item
        hook items_give
    }
    pdc {
        hook items_pdc
    }
    getRandom {
        hook items_getRandom
    }
    unlocked {
        hook items_unlocked
    }
    wipe {
        hook items_wipe
    }
}
bounty {
    help set a bounty on a player
    user player
    set player:target int:amount {
        hook bounty_set
    }
    get player:target {
        hook bounty_get
    }
    remove player:target {
        hook bounty_remove
    }
    forceremove player:target {
        hook bounty_forceremove
        permission tazpvp.staff.bounty
    }
    list {
        hook bounty_list
    }
}
kit player:target?(context self){
